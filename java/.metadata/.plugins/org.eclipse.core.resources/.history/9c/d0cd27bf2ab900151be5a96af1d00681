package model;

public class Book extends Document {
	public String author;

	public String getAuthor() {
		return author;
	}

	public void setAuthor(String author) {
		this.author = author;
	}

	@Override
	public String toString() {
		return "Book [title = "+ title +", author=" + author + ", available=" + available + "]";
	}
	
}
