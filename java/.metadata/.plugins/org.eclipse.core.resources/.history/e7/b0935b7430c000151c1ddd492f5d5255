package Components;
import java.awt.Color;
import java.awt.Component;
import java.awt.GridLayout;

import javax.swing.*;

public class CrypterPanel extends JPanel {

	private static final long serialVersionUID = 9131970873800152347L;

	public CrypterPanel() {
		this.setLayout(new BoxLayout(this, BoxLayout.PAGE_AXIS));
			JTextField cfTextField = new JTextField();
			cfTextField.setSize(250, 20);
			this.setBackground(Color.GREEN);
			this.add(new JButton("Crypter"));
			this.add(cfTextField);
		
		JPanel app = new JPanel();
		app.add(this);
		
		
		this.setVisible(true);
		System.out.println("run");
	}
}
